<?xml version="1.0"?>
	<import-steps>
		
		<import-step id="vindula.controlpanel.installControlPanel" 
					 version="1.0"
	                 handler="vindula.controlpanel.setuphandlers.installControlPanel"
	                 title="Create Vindula Control Panel Objects">
			<dependency step="content"/>
			<!--dependency step="typeinfo"/-->
		</import-step>

		<import-step id="vindula.controlpanel.link_user_folder" 
					 version="1.0"
	                 handler="vindula.controlpanel.setuphandlers.link_user_folder"
	                 title="Create Vindula Control Panel Folder User">
	    	<dependency step="content"/>
			<!--dependency step="typeinfo"/-->
		</import-step>
		
		<import-step id="vindula.controlpanel.CreateForderImage" 
					 version="1.0"
	                 handler="vindula.controlpanel.setuphandlers.CreateForderImage"
	                 title="Create Vindula Data Base Imagens">
	    	<dependency step="workflow"/>
		</import-step>	
		
		<import-step id="vindula.controlpanel.updateTopicsControlPanel" 
					 version="1.0"
	                 handler="vindula.controlpanel.setuphandlers.updateTopicsControlPanel"
	                 title="Create Vindula Control Panel Object">
	        <dependency step="content"/>
		</import-step>
		
		<import-step id="vindula.controlpanel.installSteps" 
                     version="1.0"
                     handler="vindula.controlpanel.setuphandlers.installSteps"
                     title="Create Vindula Steps Object">
            <dependency step="content"/>
        </import-step>
		
		
	</import-steps>