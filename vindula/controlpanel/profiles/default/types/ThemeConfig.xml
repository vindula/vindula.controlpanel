<?xml version="1.0"?>
<object name="ThemeConfig" 
   	 	meta_type="Factory-based Type Information with dynamic views"
		xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 
 <!-- Basic metadata -->
 <property name="title" i18n:translate="">vindual_themeconfig</property>
 <property name="description" i18n:translate="">Área para configurações principais do Portal.</property>
 <property name="content_icon">++resource++vindula.content.images/folder.gif</property>
 <property name="content_meta_type">ThemeConfig</property>
 <property name="product">vindula.controlpanel</property>
 <property name="factory">addThemeConfig</property>
  <property name="allow_discussion">False</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">False</property>
 <property name="allowed_content_types" />
 
 <property name="immediate_view">edit</property>
 <property name="default_view">edit</property>
 
 <property name="default_view_fallback">False</property>
 
 <!--alias from="(Default)" to="vindula_themeconfig"/-->
 <alias from="index.html" to="(dynamic view)"/>
 <alias from="view" to="(selected layout)"/>
 <alias from="edit" to="base_edit"/>
 
 <action title="Edit" 
         action_id="edit"
         category="object" 
         condition_expr=""
         url_expr="string:${object_url}/edit" 
         visible="True">
  <permission value="Modify portal content"/>
 </action>

</object>