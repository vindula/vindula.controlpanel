         xmlns:tal="http://xml.zope.org/namespaces/tal"
         xmlns:metal="http://xml.zope.org/namespaces/metal"
         xmlns:i18n="http://xml.zope.org/namespaces/i18n"
         lang="en"
         metal:use-macro="context/prefs_main_template/macros/master"
         i18n:domain="plone">
   
   <head>

  </head>  
  <body>
  <metal:main fill-slot="prefs_configlet_content">
  	<div class="controlpanel-links" id="content">
  		
		<div class="titulo_info">
			<h2>Vindula Control Panel</h2>
		</div>
		
		<!-- Control Panel Objects -->
		<div>
			<img src="/++resource++vindula.controlpanel/icons/controlpanel_theme.png" />
			<a tal:attributes="href string:${portal_url}/control-panel-objects/vindula_themeconfig/edit">Configura&ccedil;&otilde;es do Tema</a>
		</div>
		
		<div>
			<img src="/++resource++vindula.controlpanel/icons/controlpanel_categories.png" />
