<div class="read_more" tal:define="read_more python: viewlet.getReadMore()[:5]" tal:condition="read_more">
	<div class="titulo_area">
		<h2>Saiba Mais</h2>
	</div>
	<tal:rep repeat="new read_more">
		<div class="descricao_titulo newSaibaMais">
			<h4 tal:content="python: new.bobobase_modification_time().strftime('%d/%m/%Y ')" class="float-left">Data de Publicao</h4>
			<label class="lineHeight16"><a tal:content="string: - ${new/Title}" tal:attributes="href new/getURL; alt new/Title; title new/Title;"></a></label>
		</div>
	</tal:rep>
</div>